{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": ".",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"paths": {
			"@/*": ["src/*"]
		},
		"types": ["bun-types"],
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "dist"]
}
